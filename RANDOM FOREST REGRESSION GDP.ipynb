from sklearn.linear_model import LinearRegression

# Define the continents and their respective countries
continents_to_predict = {
    "Asia": asian_countries,
    "Africa": african_countries,
    "Europe": european_countries,
    "South America": south_american_countries
}

# Initialize a dictionary to store predictions
continent_predictions = {}

# Loop through each continent and predict GDP values
for continent, countries in continents_to_predict.items():
    continent_predictions[continent] = {}
    for country in countries:
        # Filter data for the specific country
        country_data = df[df['country_name'] == country]
        if country_data.empty:
            print(f"No data available for {country} in {continent}")
            continue
        
        # Extract years and GDP values
        years = country_data.columns[2:-1]  # Exclude 'country_name', 'indicator_name', and 'Continent'
        gdp_values = country_data.iloc[:, 2:-1].values.flatten()  # Exclude 'Continent' column
        
        # Clean data - remove NaN values
        valid_data = [(int(year), gdp) for year, gdp in zip(years, gdp_values) if not np.isnan(gdp)]
        if len(valid_data) < 3:  # Need at least 3 data points
            print(f"Insufficient data for {country} in {continent}")
            continue
        
        # Prepare features with enhanced temporal information
        years_clean = np.array([d[0] for d in valid_data])
        gdp_clean = np.array([d[1] for d in valid_data])
        
        # Create enhanced features
        X = []
        for i, year in enumerate(years_clean):
            features = [
                year,  # Base year
                year - years_clean[0],  # Years since start
                i + 1,  # Time index
            ]
            
            # Add lagged GDP values (previous years' GDP)
            if i > 0:
                features.append(gdp_clean[i-1])  # Previous year GDP
                features.append((gdp_clean[i] - gdp_clean[i-1]) / gdp_clean[i-1] * 100 if gdp_clean[i-1] != 0 else 0)  # Growth rate
            else:
                features.append(gdp_clean[0])  # Use first value
                features.append(0)  # No growth rate for first year
            
            # Add moving average if enough data
            if i >= 2:
                features.append(np.mean(gdp_clean[max(0, i-2):i+1]))  # 3-year moving average
            else:
                features.append(gdp_clean[:i+1].mean())
            
            # Add trend indicator (linear trend coefficient)
            if i >= 2:
                recent_years = years_clean[max(0, i-2):i+1]
                recent_gdp = gdp_clean[max(0, i-2):i+1]
                z = np.polyfit(recent_years, recent_gdp, 1)
                features.append(z[0])  # Trend slope
            else:
                features.append(0)
            
            X.append(features)
        
        X = np.array(X)
        y = gdp_clean
        
        # Train Random Forest model with optimized parameters
        rf_regressor = RandomForestRegressor(
            n_estimators=100,
            max_depth=10,
            min_samples_split=2,
            min_samples_leaf=1,
            random_state=42
        )
        rf_regressor.fit(X, y)
        
        # Calculate recent trend for future predictions
        recent_years = years_clean[-3:] if len(years_clean) >= 3 else years_clean
        recent_gdp = gdp_clean[-3:] if len(gdp_clean) >= 3 else gdp_clean
        
        # Fit a linear trend to recent data
        trend_model = LinearRegression()
        trend_model.fit(recent_years.reshape(-1, 1), recent_gdp)
        trend_slope = trend_model.coef_[0]
        
        # Calculate recent growth rate
        if len(gdp_clean) >= 2:
            recent_growth = (gdp_clean[-1] - gdp_clean[-2]) / gdp_clean[-2] * 100 if gdp_clean[-2] != 0 else 0
        else:
            recent_growth = 0
        
        # Prepare features for future predictions
        future_years = [2025, 2026, 2027]
        predictions = []
        
        last_gdp = gdp_clean[-1]
        last_moving_avg = np.mean(gdp_clean[-3:]) if len(gdp_clean) >= 3 else gdp_clean.mean()
        
        for i, future_year in enumerate(future_years):
            # Create features for future year
            future_features = [
                future_year,
                future_year - years_clean[0],
                len(years_clean) + i + 1,
                predictions[-1] if i > 0 else last_gdp,  # Use previous prediction or last known GDP
                recent_growth * (0.9 ** i),  # Decay growth rate slightly
                last_moving_avg if i == 0 else np.mean([last_gdp] + predictions),  # Moving average
                trend_slope  # Keep trend slope
            ]
            
            # Make prediction
            pred = rf_regressor.predict([future_features])[0]
            
            # Apply a slight trend adjustment to ensure variation
            # This combines RF prediction with trend projection
            trend_adjustment = trend_slope * (i + 1) * 0.3  # 30% weight to linear trend
            pred_adjusted = pred * 0.7 + (last_gdp + trend_slope * (i + 1)) * 0.3
            
            # Add small random walk component for realistic variation
            # (optional - remove if you want purely deterministic predictions)
            np.random.seed(42 + i + hash(country) % 1000)  # Consistent randomness per country
            variation = np.random.normal(0, abs(pred_adjusted) * 0.01)  # 1% standard deviation
            pred_adjusted += variation
            
            predictions.append(pred_adjusted)
            
            # Update recent growth for next iteration
            recent_growth = (pred_adjusted - (predictions[-2] if i > 0 else last_gdp)) / (predictions[-2] if i > 0 else last_gdp) * 100
        
        # Store predictions
        continent_predictions[continent][country] = np.array(predictions)

